{
  "name": "urban-scraper",
  "version": "1.1.1",
  "description": "Scraper for the Urban Dictionary.",
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/brunolepis/urban-scraper",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs",
    "default": "./dist/esm/index.mjs"
  },
  "scripts": {
    "build": "esbuild --out-extension:.js=.cjs --outdir=dist/cjs --platform=node --format=cjs src/index.ts --tsconfig=tsconfig.cjs.json && esbuild --out-extension:.js=.mjs --outdir=dist/esm --platform=node --format=esm src/index.ts --tsconfig=tsconfig.mjs.json && tsc --emitDeclarationOnly -p tsconfig.mjs.json",
    "dev": "ts-node -P tsconfig.mjs.json ./src/index.ts"
  },
  "keywords": [
    "scraper",
    "scrape",
    "urban-dictionary",
    "urban"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.1.5",
    "@types/node-fetch": "^2.6.4",
    "esbuild": "^0.17.19",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.1"
  }
}
